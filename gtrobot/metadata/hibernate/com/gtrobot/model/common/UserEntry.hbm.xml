<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USER_ENTRY" name="com.gtrobot.model.common.UserEntry">
    <id column="USER_ID" access="property" name="userId">
      <generator class="native">
        <param name="sequence">SEQ_USER_ENTRY</param>
        <param name="identity"/>
      </generator>
    </id>
    <property name="sysMessageEnable" not-null="true" access="property" column="SYS_MSG_ENABLE"/>
    <property name="jid" not-null="true" length="100" access="property" column="JID" unique="true"/>
    <property name="nickName" not-null="true" length="100" access="property" column="NICK_NAME" unique="true"/>
    <property name="echoable" access="property" column="ECHOABLE"/>
    <property name="locale" not-null="true" length="3" access="property" column="LOCALE"/>
    <property name="accountTypeCode" not-null="true" access="property" column="ACCOUNT_TYPE"/>
  </class>
</hibernate-mapping>
