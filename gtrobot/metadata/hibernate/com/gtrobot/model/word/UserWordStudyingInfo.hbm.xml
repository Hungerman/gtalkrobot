<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class table="USER_WORD_STUDYING_INFO" name="com.gtrobot.model.word.UserWordStudyingInfo">
    <id column="USER_ENTRY_ID" access="property" name="userId">
      <generator class="assigned"/>
    </id>
    <property name="studyingType" not-null="true" access="property" column="STUDYING_TYPE"/>
    <property name="finishedUnits" not-null="true" access="property" column="FINISHED_UNITS"/>
    <property name="lastStudied" not-null="true" access="property" column="LAST_STUDIED"/>
    <property name="studyingWordUnitId" access="property" column="STUDYING_WORD_UNIT_ID"/>
    <property name="privateWordUnitId" access="property" column="PRIVATE_WORD_UNIT_ID"/>
  </class>
</hibernate-mapping>
